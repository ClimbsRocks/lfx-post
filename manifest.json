{
  "name": "Share with Letsfix.net",
  "description": "Post the current URL to letsfix.net",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
      "default_title": "LFx this page.",
      "default_icon": "print_16x16.png"
  },
  "manifest_version": 1
}
